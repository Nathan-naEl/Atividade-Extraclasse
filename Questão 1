#include <stdio.h>

float media(int v[], int t) {
    int i;
    float soma = 0;
    for (i = 0; i < t; i++) {
        soma += v[i];
    }
    return soma / t;
}

int maior_valor(int v[], int t) {
    int i, maior = v[0];
    for (i = 1; i < t; i++) {
        if (v[i] > maior) maior = v[i];
    }
    return maior;
}

int menor_valor(int v[], int t) {
    int i, menor = v[0];
    for (i = 1; i < t; i++) {
        if (v[i] < menor) menor = v[i];
    }
    return menor;
}

int contar_pares(int v[], int t) {
    int i, pares = 0;
    for (i = 0; i < t; i++) {
        if (v[i] % 2 == 0) pares++;
    }
    return pares;
}

int main() {
    int v[10];
    int i;

    for (i = 0; i < 10; i++) {
        scanf("%d", &v[i]);
    }

    printf("Media: %.2f\n", media(v, 10));
    printf("Maior: %d\n", maior_valor(v, 10));
    printf("Menor: %d\n", menor_valor(v, 10));
    printf("Pares: %d\n", contar_pares(v, 10));

    return 0;
}
