#include <stdio.h>

int ano_bissexto(int ano) {
    if (ano % 400 == 0) return 1;
    if (ano % 100 == 0) return 0;
    if (ano % 4 == 0) return 1;
    return 0;
}

int main() {
    int ano;
    scanf("%d", &ano);

    if (ano_bissexto(ano)) {
        printf("Ano bissexto\n");
    } else {
        printf("Nao e bissexto\n");
    }

    return 0;
}
